{
  "name": "try-stream-push",
  "version": "1.1.0",
  "description": "Push the return value of the function to the stream, or make it emit the thrown error",
  "repository": "shinnn/try-stream-push",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs index.js test.js && eslint index.js test.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/try-stream-push/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "try",
    "catch",
    "push",
    "function",
    "return",
    "emit",
    "error",
    "stream",
    "read"
  ],
  "devDependencies": {
    "eslint": "^0.19.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "jscs": "^1.12.0",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "through2": "^0.6.5"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
